            <td><input type="text" value="<%=datos.VehiCatId%>" name="categoria" id="cat" class="cat"></td>
            <td><input type="text" value="<%=datos.VehiModelo%>" name="modelo" id="mod" class="mod"></td>
            <td><input type="text" value="<%=datos.VehiMarca%>" name="marca" id="mar" class="mar"></td>
            <td><input type="text" value="<%=datos.VehiColor%>" name="color" id="cor" class="cor"></td>
            <td><input type="text" value="<%=datos.VehiEstado%>" name="estado" id="est" class="est"></td>
            <td><input type="text" value="<%=datos.VehiPrecio%>" name="precio" id="pre" class="pre"></td>
          </tr>
       <%})%>
