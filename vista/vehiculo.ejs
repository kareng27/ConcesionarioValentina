          </div>
    
          
        </nav>
      </div>

      <div class="cate"><a href="/categoria">Ver Categorias de los Vehiculos</a></div>

    <section>
    <div class="entry-form2">
        <form action="/frminsertarvehiculo" method="POST">
             <h2>Registrar Vehículo</h2><br> 									
             <br><input type="text" name="VehiPlaca" class="form-control2" placeholder="Placa" required="">
             <input type="text" name="VehiDatId" class="form-control2" placeholder="Numero" required=""> 
             <input type="text" name="VehiCatId" class="form-control2" placeholder="Categoria" required=""> 
             <input type="text" name="VehiModelo" class="form-control2" placeholder="Modelo" required="">
             <input type="text" name="VehiMarca" class="form-control2" placeholder="Marca" required="">
             <input type="text" name="VehiColor" class="form-control2" placeholder="Color" required="">
             <input type="text" name="VehiEstado" class="form-control2" placeholder="Estado" required="">
             <input type="text" name="VehiPrecio" class="form-control2" placeholder="Precio del vehículo" required="">
                       
             <button class="submit-btn form-control" id="form-submit">Enviar</button>
             
            </form>
   </div>

</section>

<from action="" name="frm" id="frm">
  <table class="table80">
