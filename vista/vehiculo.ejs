              <th>CATEGORIA</th>
              <th>MODELO</th>
              <th>MARCA</th>
              <th>COLOR</th>
              <th>ESTADO</th>
              <th>PRECIO</th>
          </tr>
      </thead>

      <!<tbody>
       <% datos.forEach(function (datos){ %>    
          <tr> 									
            <td><input type="text" value="<%=datos.VehiPlaca%>" name="placa" id="pla" class="pla"></td>
